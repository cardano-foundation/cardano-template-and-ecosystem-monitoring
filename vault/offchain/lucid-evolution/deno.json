{
  "tasks": {
    "dev": "deno run --watch vault.ts",
    "init": "deno run -A vault.ts init",
    "lock": "deno run -A vault.ts lock",
    "withdraw": "deno run -A vault.ts withdraw",
    "finalize": "deno run -A vault.ts finalize",
    "cancel": "deno run -A vault.ts cancel",
    "prepare": "deno run -A vault.ts prepare"
  },
  "imports": {
    "@evolution-sdk/lucid": "npm:@evolution-sdk/lucid@^2.0.1",
    "@std/assert": "jsr:@std/assert@1"
  }
}
