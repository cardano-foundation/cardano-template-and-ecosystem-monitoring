    "prepare": "deno run -A --unstable-detect-cjs vault.ts prepare"
  },
  "imports": {
    "@meshsdk/common": "npm:@meshsdk/common@^1.9.0-beta.94",
    "@meshsdk/core": "npm:@meshsdk/core@^1.9.0-beta.94",
    "@meshsdk/core-cst": "npm:@meshsdk/core-cst@^1.9.0-beta.94",
    "@std/assert": "jsr:@std/assert@1"
  },
  "nodeModulesDir": "auto"
}
